{
  "compilerOptions": {
    "allowJs": false,
    "alwaysStrict": true,
    "declaration": false,
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "esnext"
    ],
    "newLine": "lf",
    "target": "es2015",
    "baseUrl": "./",
    "outDir": "../../lib/tests/",
    "preserveConstEnums": true,
    "removeComments": false,
    "strict": true,
    "noImplicitAny": false,
    "esModuleInterop": true
  },
  "include": [
    "./*.ts"
  ]
}
